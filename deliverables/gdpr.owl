        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital32"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital42"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Informed</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/GDPRtEXT#IntegrityAndConfidentiality -->

    <owl:Class rdf:about="https://w3id.org/GDPRtEXT#IntegrityAndConfidentiality">
        <rdfs:subClassOf rdf:resource="https://w3id.org/GDPRtEXT#Principle"/>
        <involves rdf:resource="https://w3id.org/GDPRtEXT#DataSecurity"/>
        <involves rdf:resource="https://w3id.org/GDPRtEXT#PersonalData"/>
        <involves rdf:resource="https://w3id.org/GDPRtEXT#Processing"/>
        <involves rdf:resource="https://w3id.org/GDPRtEXT#ProtectionAgainstAccidentalLoss"/>
        <involves rdf:resource="https://w3id.org/GDPRtEXT#ProtectionAgainstDamage"/>
        <involves rdf:resource="https://w3id.org/GDPRtEXT#ProtectionAgainstDestruction"/>
        <rdfs:comment xml:lang="en">The principle of integrity and confidentiality states that personal data must be  processed in a manner that ensures appropriate security of the personal data, including protection against unauthorised or unlawful processing and against accidental loss, destruction or damage, using appropriate technical or organisational measures.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#article24-1"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#article25-1"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#article25-2"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#article28"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#article32"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#article39"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#article5-1-f"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital156"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital29"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital71"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Principle of Integrity and Confidentiality</rdfs:label>
    </owl:Class>
    


