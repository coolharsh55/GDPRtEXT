        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#article9-4"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Exempted by National Law</rdfs:label>
