        <involves rdf:resource="https://w3id.org/GDPRtEXT#Processing"/>
        <rdfs:comment xml:lang="en">Given Consent refers specifically to the form of consent given by the data subject in relation to their personal data and the proposed usage by activities.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#article6-1-a"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#article9-2-a"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital32"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital42"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital43"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Given Consent</rdfs:label>
    </owl:Class>
    


