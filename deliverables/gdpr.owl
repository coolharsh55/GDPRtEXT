        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital58"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital60"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Right to Transparency</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/GDPRtEXT#Rights -->

    <owl:Class rdf:about="https://w3id.org/GDPRtEXT#Rights">
