        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital79"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Joint Controller</rdfs:label>
    </owl:Class>
    


