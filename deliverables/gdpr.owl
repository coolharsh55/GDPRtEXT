        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Exceptions on reporting data breach</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/GDPRtEXT#ExclusionException -->

    <owl:Class rdf:about="https://w3id.org/GDPRtEXT#ExclusionException">
