        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Information about processing</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/GDPRtEXT#InfoAboutSourceOfData -->

    <owl:Class rdf:about="https://w3id.org/GDPRtEXT#InfoAboutSourceOfData">
