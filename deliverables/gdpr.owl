        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#article4-12"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Data Breach</rdfs:label>
    </owl:Class>
    


