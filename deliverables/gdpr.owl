        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital73"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Right to rectify</rdfs:label>
    </owl:Class>
    


