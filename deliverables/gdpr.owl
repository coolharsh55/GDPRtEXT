        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Exlcusions and Exceptions</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/GDPRtEXT#ExemptedByNationalLaw -->

    <owl:Class rdf:about="https://w3id.org/GDPRtEXT#ExemptedByNationalLaw">
