        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#article28-3-b"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#article29"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Impose confidentiality obligations on personnel</rdfs:label>
    </owl:Class>
    


