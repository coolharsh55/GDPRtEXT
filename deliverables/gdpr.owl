        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital90"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Impact Assessment</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/GDPRtEXT#ImplementTechnicalMeasuresForCompliance -->

