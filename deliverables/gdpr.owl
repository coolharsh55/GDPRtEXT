        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital65"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital66"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital68"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Right of Erasure</rdfs:label>
    </owl:Class>
    


