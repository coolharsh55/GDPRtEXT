        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital35"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital51"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sensitive Personal Data</rdfs:label>
