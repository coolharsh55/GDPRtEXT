        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#article28-4"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#article32"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital83"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Data Security</rdfs:label>
    </owl:Class>
    


