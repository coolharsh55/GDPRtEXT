        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#article35"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital84"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital90"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital91"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital92"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital93"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital94"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Factors for Impact Assessment</rdfs:label>
