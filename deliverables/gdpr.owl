        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#article9-2-j"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital156"/>
