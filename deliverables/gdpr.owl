        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Existence of safeguards</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/GDPRtEXT#ExplicitPurpose -->

    <owl:Class rdf:about="https://w3id.org/GDPRtEXT#ExplicitPurpose">
