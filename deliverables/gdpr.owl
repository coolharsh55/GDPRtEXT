        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital86"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital87"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/GDPRtEXT/gdpr#recital88"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Report Data Breach</rdfs:label>
    </owl:Class>
    


