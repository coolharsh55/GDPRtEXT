        <eli:is_part_of rdf:resource="http://purl.org/adaptcentre/openscience/resources/DPD#chapterIII"/>
        <GDPRtext:isPartOfChapter rdf:resource="http://purl.org/adaptcentre/openscience/resources/DPD#chapterIII"/>
        <rdf:type rdf:resource="http://purl.org/adaptcentre/openscience/ontologies/GDPRtEXT#Article"/>
        <eli:number rdf:datatype="http://www.w3.org/2001/XMLSchema#string">24</eli:number>
        <eli:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sanctions</eli:title>
        <eli:title_alternative rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Article 24</eli:title_alternative>
      </eli:LegalResourceSubdivision>
    </GDPRtext:isPartOfArticle>
    <eli:title_alternative rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Article24(1)</eli:title_alternative>
    <eli:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The Member States shall adopt suitable measures to ensure the full implementation of the provisions of this Directive and shall in particular lay down the sanctions to be imposed in case of infringement of the provisions adopted pursuant to this Directive.</eli:description>
  </GDPRtext:Point>
